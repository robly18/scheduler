<html>
	<body>
		<p id ="day"></p>
		<p id ="startHour"></p>
		<p id ="endHour"></p>
		<p id ="desc"></p>
	</body>
	
	<script>
		var dict = [];
		dict.push({day: 1, startHour: 7, endHour: 8, desc: "Teste"});	
		document.getElementById("day").innerHTML = dict[0].day;
		document.getElementById("startHour").innerHTML = dict[0].startHour;
		document.getElementById("endHour").innerHTML = dict[0].endHour;
		document.getElementById("desc").innerHTML = dict[0].desc;
	</script>
</html>